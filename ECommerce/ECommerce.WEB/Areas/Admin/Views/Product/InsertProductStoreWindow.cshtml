@model ECommerce.WEB.Areas.Admin.Model.ProductManagement.ProductStoreWindowCRUDModel
@{
    ViewBag.Title = "InsertProductStoreWindow";
}




@using (Html.BeginForm("InsertProductStoreWindow", "Product", FormMethod.Post, new { @id = "InsertProductStoreWindow" }))
{



    <div class="form-group">

        @Html.LabelFor(s => s.ProductListId, new { @class = "col-form-label" })
        @Html.DropDownListFor(s => s.ProductListId, Model.ProductList, string.Empty, new { @class = "form-control",multiple="" })
        @Html.ValidationMessageFor(s => s.ProductListId)
    </div>


    <div class="form-group">

        <input type="submit" class="btn btn-success" value="Kaydet" />
    </div>

}


@section Scripts{
    <script>

        Jform("#InsertProductStoreWindow");


    </script>

}
